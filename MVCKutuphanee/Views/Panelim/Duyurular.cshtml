@using MVCKutuphanee.Models.Entity
@model List<TBLDUYURULAR>
@{
    ViewBag.Title = "Duyurular";
    Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}
<title>Duyurular</title>
<br />
<h2>Duyurular</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>Kategori</th>
        <th>Tarih</th>
        <th>İçerik</th>
    </tr>
    @foreach (var prs in Model)
    {
<tr>
    <td>@prs.KATEGORI</td>
    <td>@Convert.ToDateTime(prs.TARIH).ToString("dd/MM/yyyy")</td>
    <td>@prs.ICERIK</td>


</tr>
    }
</table>

